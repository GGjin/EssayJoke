<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="18.0dip" />
            <gradient android:startColor="#fffff8dc" android:endColor="#ffeee8cd" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffcdc8b1" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="18.0dip" />
                <gradient android:startColor="#ff1e90ff" android:endColor="#ff1e90ff" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff1e90ff" />
            </shape>
        </clip>
    </item>
</layer-list>